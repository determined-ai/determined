{
  "name": "determined-webui",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=16.13 <17",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "setup": "node scripts/setupThemes.js",
    "postshrinkwrap": "npx npm-force-resolutions && npm install --ignore-scripts",
    "start": "craco start",
    "build": "craco build",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:css": "stylelint --config .stylelintrc.js \"src/**/*.{css,less,scss}\"",
    "format": "npm run format:js && npm run format:css && npm run format:misc",
    "format:js": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:css": "prettier --write \"src/**/*.{css,less,scss}\"",
    "format:misc": "prettier --write \"src/**/*.{md,json}\"",
    "format-check": "npm run format-check:js && npm run format-check:css && npm run format-check:misc",
    "format-check:js": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "format-check:css": "prettier --check \"src/**/*.{css,less,scss}\"",
    "format-check:misc": "prettier --check \"src/**/*.{md,json}\"",
    "test": "craco test --config jest.config.js",
    "test:coverage": "npm test -- --env=jsdom --coverage --reporters default --watchAll=false",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "storybook": "NODE_PATH=src start-storybook -p 9009",
    "storybook:build": "NODE_PATH=src build-storybook -o build-storybook"
  },
  "dependencies": {
    "ansi-to-html": "^0.7.1",
    "antd": "^4.21.4",
    "dayjs": "^1.10.7",
    "debug": "^4.3.4",
    "fp-ts": "^2.11.2",
    "fuse.js": "^6.4.6",
    "hermes-parallel-coordinates": "^0.5.18",
    "history": "^4.10.1",
    "humanize-duration": "^3.27.0",
    "io-ts": "^2.2.16",
    "js-sha512": "^0.8.0",
    "js-yaml": "^4.1.0",
    "markdown-to-jsx": "^7.1.3",
    "omnibar": "github:hamidzr/omnibar",
    "path-to-regexp": "^6.2.1",
    "portable-fetch": "^3.0.0",
    "pretty-bytes": "^5.6.0",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dnd": "^15.1.1",
    "react-dnd-html5-backend": "^15.1.2",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.4",
    "react-helmet-async": "^1.1.2",
    "react-ipynb-renderer": "^0.3.0",
    "react-monaco-editor": "^0.44.0",
    "react-router-dom": "^5.3.0",
    "react-router-dom-v5-compat": "^6.3.0",
    "react-transition-group": "^4.4.2",
    "react-window": "^1.8.6",
    "screenfull": "^5.1.0",
    "smoothscroll-polyfill": "^0.4.4",
    "sprintf-js": "^1.1.2",
    "throttle-debounce": "^5.0.0",
    "uplot": "^1.6.18"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.4.0",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-backgrounds": "^6.5.9",
    "@storybook/addon-docs": "^6.5.9",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-toolbars": "^6.5.9",
    "@storybook/addon-viewport": "^6.5.9",
    "@storybook/addons": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.2.1",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/js-yaml": "^4.0.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-transition-group": "^4.4.0",
    "@types/react-window": "^1.8.5",
    "@types/resize-observer-browser": "^0.1.4",
    "@types/segment-analytics": "0.0.33",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@types/sprintf-js": "^1.1.2",
    "@types/throttle-debounce": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "antd-dayjs-webpack-plugin": "^1.0.1",
    "antd-theme-webpack-plugin": "^1.3.6",
    "babel-plugin-import": "^1.13.1",
    "craco-less": "^1.20.0",
    "craco-sass-resources-loader": "^1.1.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "express": "^4.17.1",
    "jest-junit": "^13.0.0",
    "less": "^4.1.1",
    "less-loader": "^7.0.2",
    "monaco-editor-webpack-plugin": "^3.0.1",
    "morgan": "^1.10.0",
    "npm-force-resolutions": "0.0.10",
    "prettier": "^2.7.1",
    "react-app-rewired": "^2.1.6",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-scripts": "^4.0.3",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.35.2",
    "sass-loader": "^10.1.1",
    "source-map-explorer": "^2.5.0",
    "storybook-preset-craco": "0.0.6",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "typescript": "^4.7.3"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/src/services/api-ts-sdk/",
      ".stories.tsx"
    ],
    "resetMocks": false
  }
}
