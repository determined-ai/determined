{
  "name": "determined-webui",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=16.19 <20",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "postshrinkwrap": "npx npm-force-resolutions && npm install --ignore-scripts",
    "start": "vite serve",
    "preview": "vite preview",
    "build": "NODE_OPTIONS=--max-old-space-size=6144 vite build --mode=${DET_NODE_ENV:-production}",
    "prettier": "prettier",
    "eslint": "eslint",
    "stylelint": "stylelint --config .stylelintrc.js",
    "lint:eslint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:style": "stylelint --config .stylelintrc.js \"src/**/*.{css,less,scss}\"",
    "lint": "npm run eslint && npm run stylelint && npm run format-check",
    "format": "npm run format:js && npm run format:css && npm run format:misc",
    "format:js": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:css": "prettier --write \"src/**/*.{css,less,scss}\"",
    "format:misc": "prettier --write \"src/**/*.{md,json}\"",
    "format-check": "npm run format-check:js && npm run format-check:css && npm run format-check:misc",
    "format-check:js": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "format-check:css": "prettier --check \"src/**/*.{css,less,scss}\"",
    "format-check:misc": "prettier --check \"src/**/*.{md,json}\"",
    "test": "vitest",
    "e2e": "playwright test",
    "test:coverage": "vitest --run --coverage --silent",
    "analyze": "source-map-explorer --no-border-checks 'build/assets/*.js'"
  },
  "dependencies": {
    "@determined-ai/hermes-parallel-coordinates": "^0.6.1",
    "@glideapps/glide-data-grid": "^5.2.1",
    "ansi-to-html": "^0.7.2",
    "antd": "^5.1.7",
    "dayjs": "^1.11.7",
    "debug": "^4.3.4",
    "events": "^3.3.0",
    "fp-ts": "^2.13.1",
    "fuse.js": "^6.6.2",
    "humanize-duration": "^3.28.0",
    "immutable": "^4.3.0",
    "io-ts": "^2.2.20",
    "js-sha512": "^0.8.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.1.9",
    "marked": "^4.3.0",
    "micro-observables": "^1.7.2",
    "notebook": "file:src/vendor/notebook",
    "omnibar": "github:hamidzr/omnibar",
    "path-to-regexp": "^6.2.1",
    "pretty-bytes": "^6.1.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "react-helmet-async": "^1.3.0",
    "react-monaco-editor": "^0.44.0",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^6.9.0",
    "react-transition-group": "^4.4.5",
    "react-window": "^1.8.8",
    "screenfull": "^6.0.2",
    "smoothscroll-polyfill": "^0.4.4",
    "sprintf-js": "^1.1.2",
    "throttle-debounce": "^5.0.0",
    "uplot": "^1.6.18"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.32.3",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/humanize-duration": "^3.27.1",
    "@types/js-yaml": "^4.0.5",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.8",
    "@types/react-transition-group": "^4.4.5",
    "@types/react-window": "^1.8.5",
    "@types/resize-observer-browser": "^0.1.7",
    "@types/segment-analytics": "0.0.34",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@types/sprintf-js": "^1.1.2",
    "@types/throttle-debounce": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "@vitest/coverage-c8": "^0.29.3",
    "browserslist": "^4.21.5",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^40.0.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "express": "^4.18.2",
    "jsdom": "^16.7.0",
    "magic-string": "^0.30.0",
    "morgan": "^1.10.0",
    "npm-force-resolutions": "0.0.10",
    "prettier": "^2.8.4",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.59.3",
    "source-map-explorer": "^2.5.3",
    "stylelint": "^15.2.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^4.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-checker": "^0.5.6",
    "vite-tsconfig-paths": "^4.0.7",
    "vitest": "^0.29.3",
    "whatwg-fetch": "^3.6.2"
  }
}
