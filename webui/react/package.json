{
  "name": "determined-webui",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=16.13 <20",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "postshrinkwrap": "npx npm-force-resolutions && npm install --ignore-scripts",
    "start": "vite serve",
    "build": "NODE_OPTIONS=--max-old-space-size=6144 vite build --mode=${DET_NODE_ENV:-production}",
    "prettier": "prettier",
    "eslint": "eslint",
    "stylelint": "stylelint --config .stylelintrc.js",
    "lint:eslint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:style": "stylelint --config .stylelintrc.js \"src/**/*.{css,less,scss}\"",
    "lint": "npm run eslint && npm run stylelint && npm run format-check",
    "format": "npm run format:js && npm run format:css && npm run format:misc",
    "format:js": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:css": "prettier --write \"src/**/*.{css,less,scss}\"",
    "format:misc": "prettier --write \"src/**/*.{md,json}\"",
    "format-check": "npm run format-check:js && npm run format-check:css && npm run format-check:misc",
    "format-check:js": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "format-check:css": "prettier --check \"src/**/*.{css,less,scss}\"",
    "format-check:misc": "prettier --check \"src/**/*.{md,json}\"",
    "test": "jest --config ./jest.config.json",
    "test:coverage": "npm test -- --env=jsdom --coverage --reporters default --watchAll=false",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer --no-border-checks 'build/assets/*.js'"
  },
  "dependencies": {
    "@determined-ai/hermes-parallel-coordinates": "^0.6.1",
    "ansi-to-html": "^0.7.1",
    "antd": "^5.1.7",
    "dayjs": "^1.10.7",
    "debug": "^4.3.4",
    "events": "^3.3.0",
    "fp-ts": "^2.11.2",
    "fuse.js": "^6.4.6",
    "history": "^5.3.0",
    "humanize-duration": "^3.27.0",
    "immutable": "^4.1.0",
    "io-ts": "^2.2.16",
    "js-sha512": "^0.8.0",
    "js-yaml": "^4.1.0",
    "markdown-to-jsx": "^7.1.3",
    "micro-observables": "^1.7.2",
    "notebook": "file:src/vendor/notebook",
    "omnibar": "github:hamidzr/omnibar",
    "path-to-regexp": "^6.2.1",
    "portable-fetch": "^3.0.0",
    "pretty-bytes": "^5.6.0",
    "query-string": "^7.0.1",
    "react": "^18.2.0",
    "react-dnd": "^15.1.1",
    "react-dnd-html5-backend": "^15.1.2",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "react-helmet-async": "^1.3.0",
    "react-monaco-editor": "^0.44.0",
    "react-router-dom": "^6.4.2",
    "react-transition-group": "^4.4.2",
    "react-window": "^1.8.7",
    "screenfull": "^5.1.0",
    "smoothscroll-polyfill": "^0.4.4",
    "sprintf-js": "^1.1.2",
    "throttle-debounce": "^5.0.0",
    "uplot": "^1.6.18",
    "url": "^0.11.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@swc/jest": "^0.2.24",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.14",
    "@types/humanize-duration": "^3.27.1",
    "@types/jest": "^29.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.8",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.5",
    "@types/react-window": "^1.8.5",
    "@types/resize-observer-browser": "^0.1.7",
    "@types/segment-analytics": "0.0.34",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@types/sprintf-js": "^1.1.2",
    "@types/throttle-debounce": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vitejs/plugin-react-swc": "^3.1.0",
    "browserslist": "^4.21.4",
    "cheerio": "^1.0.0-rc.12",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-jsdoc": "^39.6.8",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "express": "^4.17.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.5",
    "jest-environment-jsdom": "^29.0.5",
    "jest-junit": "^13.0.0",
    "jest-transformer-svg": "^2.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "magic-string": "^0.27.0",
    "monaco-editor-webpack-plugin": "^3.0.1",
    "morgan": "^1.10.0",
    "npm-force-resolutions": "0.0.10",
    "prettier": "^2.8.0",
    "react-app-polyfill": "^3.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.35.2",
    "source-map-explorer": "^2.5.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vite": "^4.1.1",
    "vite-plugin-checker": "^0.5.5",
    "vite-tsconfig-paths": "^4.0.5"
  }
}
