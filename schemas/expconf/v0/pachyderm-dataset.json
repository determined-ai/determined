    "title": "PachydermDatasetConfig",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "project",
        "repo"
    ],
    "properties": {
        "project": {
            "type": [
                "string"
            ]
        },
        "repo": {
            "type": [
                "string"
            ]
        },
        "commit": {
            "type": [
                "string",
                "null"
            ],
